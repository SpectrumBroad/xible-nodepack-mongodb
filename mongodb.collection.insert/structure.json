{
  "name": "mongodb.collection.insert",
  "type": "action",
  "description": "Insert one or more documents into one or more MongoDb collections.",
  "inputs": {
    "trigger": {
      "type": "trigger"
    },
    "collections": {
      "type": "mongodb.collection"
    },
    "documents": {
      "type": "object"
    }
  },
  "outputs": {
    "done": {
      "type": "trigger"
    }
  }
}
